In[1547]:= A = {{1, -1, 4}, {3, 2, -1}, {2, 1, -1}};
f[t_] := {0, 0, 0};

{lambda, vecs} = Eigensystem[A];
P = Transpose[vecs];
Di = DiagonalMatrix[lambda];
Pinv = Inverse[P];

g[t_] := Pinv . f[t];

Clear[y1, y2, y3]
eqns = {y1'[t] == Di[[1, 1]] y1[t] + g[t][[1]], 
   y2'[t] == Di[[2, 2]] y2[t] + g[t][[2]], 
   y3'[t] == Di[[3, 3]] y3[t] + g[t][[3]]};

sol = DSolve[eqns, {y1, y2, y3}, t][[1]];
Y[t_] := {y1[t], y2[t], y3[t]} /. sol;
X[t_] := Simplify[P . Y[t]];

MatrixForm[X[t]]

