In[454]:= A = {{3, 2, 4}, {2, 0, 2}, {4, 2, 3}};
f[t_] := {0, 0, 0};
s =.;

eAt = InverseLaplaceTransform[Inverse[s IdentityMatrix[3] - A], s, t];
c = {C[1], C[2], C[3]};
yh = eAt . c;
yp = eAt . 
   Integrate[
    Simplify[InverseLaplaceTransform[Inverse[s IdentityMatrix[3] - A], s, t] /. 
       t -> -t] . f[t], t];

MatrixForm[Expand[yh + yp]]

